%h1 Add Manual
= form_for :manual, url: manuals_path do |f|
  %p
    = f.label :tipo
    %br/
    = f.text_field(:tipo, {:class => "form-control"})
  %p
    = f.label :marca
    %br/
    = f.text_area(:marca, {:class => "form-control"})
  %p
    = f.label :codigo
    %br/
    = f.text_area(:codigo, {:class => "form-control"})
  %p
    = f.label :modelo
    %br/
    = f.text_area(:modelo, {:class => "form-control"})
  %p
    = f.label :path
    %br/
    = f.text_area(:path, {:class => "form-control"})
  %p
    = f.submit({:class => "btn btn-primary"})

